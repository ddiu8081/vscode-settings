{
  // Workbench
  "workbench.colorTheme": "Moegi Dark",
  "workbench.iconTheme": "file-icons",
  "workbench.preferredDarkColorTheme": "Moegi Dark",
  "workbench.preferredLightColorTheme": "Moegi Light",
  "window.autoDetectColorScheme": true,
  "window.nativeTabs": true,
  "workbench.startupEditor": "none",

  // Editor
  "editor.fontFamily" : "IBM Plex Mono, Operator Mono Lig, Dank Mono, Zed Mono, Input Mono, Jetbrains Mono, Cascadia Code, Menlo, monospace",
  "editor.fontSize" : 16,
  "editor.fontWeight": "normal",
  "editor.fontLigatures": true,
  "editor.tabSize" : 2,
  "editor.inlineSuggest.enabled" : true,
  "editor.lineNumbers" : "interval",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.stickyScroll.enabled": true,
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontFamily": "Input Mono, Menlo, monospace",
  "explorer.confirmDelete" : false,
    
  // Editor: Inlay Hints
  "editor.inlayHints.enabled": "onUnlessPressed",
  "editor.inlayHints.fontSize": 12,
  "editor.inlayHints.padding": true,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "deno.inlayHints.parameterNames.enabled": "literals",

  // Extension Settings
  "volar.icon.splitEditors" : false,
  "volar.autoCompleteRefs": false,
  "prettier.tabWidth": 2,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.printWidth": 280,
  "git.autofetch" : true,
  "github.copilot.enable" : {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true,
    "vue": true
  },
  "gitlens.codeLens.enabled" : false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled" : false,
  "errorLens.fontWeight" : "600",
  "errorLens.fontSize" : "14",
  "where-am-i.colorful": false,
    
  // Theme Adjustments
  "editor.tokenColorCustomizations": {
    "[Moegi Dark][Moegi Black][Moegi Light]": {
      "textMateRules": [
        {
          "name": "Comment Italics",
          "scope": [
            "comment",
            "punctuation.definition.comment",
          ],
          "settings": {
            "fontStyle": "italic"
          }
        }
      ]
    }
  },

